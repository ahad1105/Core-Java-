// 1D array
public class hello
{
    public static void main(String args[])
    {
        int d[] =   
            {1,2,3,4};
            
    
            for(int i =0;i<3;i++)  //for loop to traverse through all the elements of an array
            {
              System.out.print(d[i]); 
            }
            
        }
    }
