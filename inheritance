//use of abstract keyword
//using abstract keyword we cannot create an object of a class

abstract class Human  
{
	public void eat()
	{
		
	}
	
	public void walk()
	{
		
	}
}

public class AbstractDemo 
{
	public static void main(String[] args)
	{
		Human obj = new Man();  //wont instantiate this object of abstract class Human
	}
}
