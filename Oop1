#classes and objects part1

class hello
{
  int i=7;
  int j=6;
  
  
  
  
    public perform(int n)
    {
        int i = n;
        int j = n;
    }
    
    public show(double d)
    {
      int i = n;
      int j = d;
    }
    
}

public class Main
{
  
  public static void main(String[] args)
  {
    hello obj1 = new hello(4);
    
    obj1.show();
    obj1.perform();
    System.out.println(obj1.i);
    System.out.println(obj1.j);
  }
}

    
