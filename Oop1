#classes and objects part1

class hello
{
  int i=7;
  int j=6;
  
  
  
  
    public hello(int n) //default constructor 
    {
        int i = n;
        int j = n;
    }
    
    public hello(double d) //1st constructor
    {
      int i = n;
      double j = d;
    }
    
    public hello(int n, double d)
    {
      int i = n;
      double j = d;
    }
    
}

public class Main
{
  
  public static void main(String[] args)
  {
    hello obj1 = new hello(4,5.6);
    
    
    obj1.show();
    obj1.perform();
    System.out.println(obj1.i);
    System.out.println(obj1.j);
  }
}

    
